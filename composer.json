{
    "name": "mars/scheduler",
    "description": "Mars framework task scheduler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Houssene Dao",
            "email": "dao.houssene@themartiangeeks.com"
        },
        {
            "name": "Hassane Dao",
            "email": "dao.hassane@themartiangeeks.com"
        }
    ],
    "support": {
        "issues": "https://github.com/marsphp/scheduler/issues",
        "source": "https://github.com/marsphp/scheduler"
    },
    "require": {
        "php": ">=7.0",
        "mtdowling/cron-expression": "^1.2",
        "nesbot/carbon": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Mars\\Scheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Events\\": "events"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
